doctype html
html
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    title CodeBattles
    meta(name='description', content='')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    link(href='/css/custom.css' rel='stylesheet')
    link(href='/css/bootstrap.min.css', rel='stylesheet')
    script(src='/js/getChallengeURL.js').
  body
    div
    script(async='', defer='')
    h2 Your code has an error.

    div(class="table-responsive")
     table(class="table table-hover")
      thead
       th Test Case #
       th Status
      tbody
       - var n = 1
       - var m = numPassed+2

       while n <= numPassed
        tr
         td=n+1
         td Passed
         - n++

       tr
        td  #{n}
        td  #{error}

       while m <= numCases
        tr
         td=m
         td Not Attempted
         - m++ 

    form(name = "Try_Again", onsubmit = "return getChallengeURL()", method = "POST")
     input#username(type="hidden" value=username name="username")
     input#attemptid(type="hidden" value=AttemptId name="attemptid")
     button#try_again.btn.btn-primary(type='submit') Try Problem Again
    button#problem_list.btn.btn-danger(onclick="location.href='/problems'") Try Another Problem



    
    

